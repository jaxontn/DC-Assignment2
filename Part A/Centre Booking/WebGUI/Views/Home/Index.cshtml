@{
    @using WebGUI.Models
}
<h1>You are in body of index</h1>

<div id="tableview">
    <table>
        @foreach (var centre in Model as IEnumerable<Centre>)
        {
            <tr>
                <td>@centre.Id</td>
                <td>@centre.Name</td>
            </tr>
        }
        
    </table>
</div>